#. settings-schema.json->ai_model->options
msgid "deepseek-math"
msgstr "deepseek-math"

#. settings-schema.json->ai_model->options
msgid "claude-3-5-sonnet"
msgstr "claude-3-5-sonnet"

#. settings-schema.json->ai_model->options
msgid "claude-3-5-haiku"
msgstr "claude-3-5-haiku"

#. settings-schema.json->ai_model->options
msgid "claude-3-opus"
