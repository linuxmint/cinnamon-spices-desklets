msgstr "gpt-5"

#. settings-schema.json->ai_model->options
msgid "gpt-4o"
msgstr "gpt-4o"

#. settings-schema.json->ai_model->options
msgid "gpt-4o-mini"
msgstr "gpt-4o-mini"

#. settings-schema.json->ai_model->options
msgid "gpt-4-turbo"
msgstr "gpt-4-turbo"

#. settings-schema.json->ai_model->options
msgid "gpt-4"
msgstr "gpt-4"

#. settings-schema.json->ai_model->options
msgid "gpt-3.5-turbo"
