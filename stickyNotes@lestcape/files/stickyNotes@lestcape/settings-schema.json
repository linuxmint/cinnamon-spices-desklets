{
    "separator-general-top": {
       "type": "separator"
    },
    "header-general": {
        "type": "header",
        "description": "General:"
    },
    "separator-general-bottom": {
       "type": "separator"
    },
    "multi-instance": {
        "default": false,
        "description": "Multiple Instances.",
        "type": "checkbox",
        "tooltip": "Check this if you want to have different instances of this desklet for each note."
    },
    "auto-hide-buttons": {
        "default": false,
        "description": "Auto hide buttons.",
        "type": "checkbox",
        "tooltip": "Check this if you want to autohide the action buttons when mouse is outside of the desklet."
    },
    "show-scroll": {
        "default": true,
        "description": "Show scrolling area.",
        "type": "checkbox",
        "tooltip": "Check this if you want to show the scrolling area or not."
    },
    "auto-scroll": {
        "default": true,
        "description": "Enable autoscrolling.",
        "type": "checkbox",
        "tooltip": "Check this if you want to enable smooth autoscrolling or not."
    },
    "raise-new-note": {
        "default": false,
        "description": "Raise the new note.",
        "type": "checkbox",
        "tooltip": "Check this if you want to raise the new note or not."
    },
    "separator-applet-top": {
       "type": "separator"
    },
    "header-applet": {
        "type": "header",
        "description": "Applet Manager:"
    },
    "separator-applet-bottom": {
       "type": "separator"
    },
    "applet-manager": {
        "default": false,
        "description": "Show Applet Manager.",
        "type": "checkbox",
        "tooltip": "Check this if you want to show the Applet Manager."
    },
    "applet-collapsed": {
        "default": false,
        "description": "Collapsed Applet Manager.",
        "type": "checkbox",
        "dependency": "applet-manager",
        "indent": true,
        "tooltip": "Check this if you want to collapsed the Applet Manager."
    },
    "applet-symbolic": {
        "default": false,
        "description": "Show symbolic icons in the Applet Manager.",
        "type": "checkbox",
        "dependency": "applet-manager",
        "indent": true,
        "tooltip": "Check this if you want to show symbolic icons in the Applet Manager."
    },
    "separator-layouts-top": {
       "type": "separator"
    },
    "header-layouts": {
        "type": "header",
        "description": "Layouts:"
    },
    "separator-layouts-bottom": {
       "type": "separator"
    },
    "stripe-layout": {
        "type": "combobox",
        "description": "Selected theme of paper sheet:",
        "default": "none",
        "tooltip": "Select a theme of paper sheet to show in Notes.",
        "options": {
            "None": "none",
            "Simple Stripe": "simplestripe",
            "Double Stripe": "doublestripe",
            "Gridded Stripe": "griddedstripe"
        }
    },
    "staples-layout": {
        "type": "combobox",
        "description": "Selected theme of staples:",
        "default": "none",
        "tooltip": "Select theme of staples to show in Notes.",
        "options": {
            "None": "none",
            "Double Staple": "doublestaple",
            "Border Staple": "borderstaple",
            "Bright Staple": "brightstaple",
            "Chain Staple": "chainstaple",
            "Clamp Staple": "clampstaple",
            "Clip Staple": "clipstaple",
            "Offhanded Staple": "offhandedstaple",
            "Tack Staple": "tackstaple",
            "White Staple": "whitestaple"
        }
    },
    "pencil-layout": {
        "type": "combobox",
        "description": "Selected theme of pencil:",
        "default": "bluepencil",
        "tooltip": "Select a theme of pencil to show in Notes.",
        "options": {
            "None": "none",
            "Blue Pencil": "bluepencil",
            "Yellow Pencil": "yellowpencil",
            "Orange Pencil": "orangepencil",
            "Black Pencil": "blackpencil"
        }
    },
    "separator-shortcut-top": {
       "type": "separator"
    },
    "header-shortcut": {
        "type": "header",
        "description": "Shortcut keys:"
    },
    "separator-shortcut-bottom": {
       "type": "separator"
    },
    "raise-key": {
        "type": "keybinding",
        "description": "Shortcut key to raise the desklet to the top:",
        "default": "<Super>n"
    },
    "hide-key": {
        "type": "keybinding",
        "description": "Shortcut key to hide the desklet:",
        "default": "<Super>h"
    },
    "separator-size-top": {
       "type": "separator"
    },
    "header-size": {
        "type": "header",
        "description": "Size:"
    },
    "separator-size-bottom": {
       "type": "separator"
    },
    "fix-size": {
        "default": false,
        "description": "Fixed size.",
        "type": "checkbox",
        "tooltip": "Check this if you want to control the size of all instances."
    },
    "same-size": {
        "default": true,
        "description": "All instace have the same fixed size.",
        "type": "checkbox",
        "dependency": "fix-size",
        "tooltip": "Check this if you want to have the same fixed size on all instances."
    },
    "width": {
        "default": 220,
        "type": "spinbutton",
        "min": 200,
        "max": 1600,
        "description": "Width:",
        "units": "pixels",
        "step": 10,
        "indent": true,
        "dependency": "same-size",
        "tooltip": "Increase or decrease the desklet width."
    },
    "height": {
        "default": 120,
        "type": "spinbutton",
        "min": 80,
        "max": 500,
        "description": "Height:",
        "units": "pixels",
        "step": 10,
        "indent": true,
        "dependency": "same-size",
        "tooltip": "Increase or decrease the desklet height."
    },
    "separator-appearance-top": {
       "type": "separator"
    },
    "header-appearance": {
        "type": "header",
        "description": "Appearance:"
    },
    "separator-appearance-bottom": {
       "type": "separator"
    },
    "text-size": {
        "default": 12,
        "type": "spinbutton",
        "min": 10,
        "max": 30,
        "description": "Font size:",
        "units": "pixels",
        "step": 1,
        "tooltip": "Increase or decrease the font size of Note text."
    },
