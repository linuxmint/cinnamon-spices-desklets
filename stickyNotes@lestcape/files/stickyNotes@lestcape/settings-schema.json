    "symbolic-icons": {
        "default": true,
        "description": "Show symbolic icons.",
        "type": "checkbox",
        "tooltip": "Check this if you want to show symbolic instead full color icons."
    },
    "desklet-opacity": {
        "type": "scale",
        "default": 1.0,
        "min": 0,
        "max": 1,
        "step": 0.05,
        "description": "Opacity of the desklet:",
        "tooltip": "Select the opacity of the desklet."
    },
    "boxes-opacity": {
        "type": "scale",
        "default": 1.0,
        "min": 0,
        "max": 1,
        "step": 0.05,
        "description": "Opacity of the boxes:",
        "tooltip": "Select the opacity of the boxes."
    },
    "separator-advance-top": {
       "type": "separator"
    },
    "header-advance": {
        "type": "header",
        "description": "Advance Appearance:"
    },
    "separator-advance-bottom": {
       "type": "separator"
    },
    "override-theme": {
        "default": false,
        "description": "Override the Cinnamon theme properties.",
        "type": "checkbox",
        "tooltip": "Check this if you want to override the Cinnamon theme properties or not."
    },
    "override-text-box": {
        "default": true,
        "description": "Override all Cinnamon theme properties on the text box.",
        "type": "checkbox",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Check this if you also want to override all Cinnamon theme properties on the text box or not."
    },
    "main-box-color": {
        "type": "colorchooser",
        "default": "rgb(0,0,0)",
        "description": "Main box color:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Set the custom main box's color."
    },
    "text-box-color": {
        "type": "colorchooser",
        "default": "rgb(255,255,255)",
        "description": "Text box color:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Set the custom text box's color."
    },
    "font-color": {
        "type": "colorchooser",
        "default": "rgb(0,0,0)",
        "description": "Font color:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Set the custom text color."
    },
    "selected-text-color": {
        "type": "colorchooser",
        "default": "rgb(128,128,128)",
        "description": "Selected text color:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Set the custom selected text color."
    },
    "border-box-width": {
        "default": 1,
        "type": "spinbutton",
        "min": 0,
        "max": 4,
        "units": "pixels",
        "step": 1,
        "description": "Border's box width:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Increase or decrease the border's box width."
    },
    "border-box-color": {
        "type": "colorchooser",
        "default": "white",
        "description": "Border's box color:",
        "indent": true,
        "dependency": "override-theme",
        "tooltip": "Set the custom border box's color."
    },
