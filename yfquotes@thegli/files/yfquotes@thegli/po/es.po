#. metadata.json->description
msgid ""
"Displays financial market information such as stock quotes and commodity "
"prices from Yahoo Finance"
msgstr ""
"Muestra información sobre mercados financieros, como cotizaciones bursátiles "
"y precios de materias primas de Yahoo Finanzas"

#. metadata.json->name
msgid "Yahoo Finance Quotes"
msgstr "Cotizaciones de Yahoo Finanzas"

#. settings-schema.json->layoutPage->title
msgid "Layout"
msgstr "Disposición"

#. settings-schema.json->dataPage->title
#. settings-schema.json->dataSymbolsSection->title
msgid "Quotes"
msgstr "Cotizaciones"

#. settings-schema.json->detailsPage->title
msgid "Details"
msgstr "Detalles"

#. settings-schema.json->presentationPage->title
msgid "Presentation"
msgstr "Presentación"

#. settings-schema.json->dataUpdateSection->title
msgid "Data Updates"
msgstr "Actualizaciones de datos"

