{
    "head0": {
        "type": "header",
        "description": "Settings for cpuload@kimse"
    },

    "head1": {
        "type": "header",
        "description": "Visual"
    },
    "hide-decorations": {
        "type": "checkbox",
        "description": "Hide decorations",
        "default": false
    },
    "scale-size": {
