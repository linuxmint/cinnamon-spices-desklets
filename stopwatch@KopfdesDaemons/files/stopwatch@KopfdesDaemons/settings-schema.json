  },
  "head1": {
    "type": "header",
    "description": "Indicator"
  },
  "indicator-color": {
    "type": "colorchooser",
    "default": "rgb(51, 209, 122)",
    "description": "Indicator color"
  },
  "circle-color": {
    "type": "colorchooser",
    "default": "rgba(255, 255, 255, 0.2)",
