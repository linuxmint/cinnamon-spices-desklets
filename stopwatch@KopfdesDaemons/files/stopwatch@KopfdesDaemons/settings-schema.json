{
  "head0": {
    "type": "header",
    "description": "Style"
  },
  "label-color": {
    "type": "colorchooser",
    "default": "rgb(51, 209, 122)",
    "description": "Label color"
  },
  "scale-size": {
    "type": "scale",
    "default": 1,
    "min": 0.4,
    "max": 5,
    "step": 0.05,
    "description": "Desklet size"
  },
  "hideDecorations": {
    "type": "checkbox",
    "description": "Hide decorations",
