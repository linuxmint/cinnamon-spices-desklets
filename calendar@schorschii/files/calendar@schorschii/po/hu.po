#. calendar@schorschii->metadata.json->name
#: desklet.js:275
msgid "Calendar"
msgstr "Naptár"

#. calendar@schorschii->metadata.json->description
msgid "A calendar desklet that can display the number of today's appointments from an ical file."
msgstr "Naptár asztalkalmazás, amely megjelenítheti a mai találkozók számát egy ical fájlból."

#. calendar@schorschii->settings-schema.json->notification-color->description
msgid "Appointment number background color"
msgstr "Találkozók számának háttérszíne"

#. calendar@schorschii->settings-schema.json->notification-color->tooltip
msgid "Set the appointment number background color of this desklet."
msgstr "Állítsa be a találkozók számának háttérszínét az asztalkalmazásban."

#. calendar@schorschii->settings-schema.json->hide-decorations->description
msgid "Hide decorations"
msgstr "Dekoráció elrejtése"

#. calendar@schorschii->settings-schema.json->head3->description
msgid "Custom desklet label (only visible if decorations are enabled)"
msgstr "Egyéni asztali címke (csak akkor látható, ha a dekoráció engedélyezve van)"

#. calendar@schorschii->settings-schema.json->head2->description
msgid "Number of appointments"
msgstr "Találkozók száma"

#. calendar@schorschii->settings-schema.json->head1->description
msgid "Visual"
msgstr "Vizuális"

#. calendar@schorschii->settings-schema.json->head0->description
msgid "Settings for calendar@schorschii"
msgstr "Calendar@schorschii beállításai"

#. calendar@schorschii->settings-schema.json->bg-img->description
msgid "Calendar style"
msgstr "Naptár stílusa"

#. calendar@schorschii->settings-schema.json->bg-img->tooltip
msgid "Select the background graphic you would like to use."
msgstr "Válassza ki a használni kívánt háttérgrafikát."

#. calendar@schorschii->settings-schema.json->bg-img->options
msgid "Orange"
msgstr "Narancssárga"

#. calendar@schorschii->settings-schema.json->bg-img->options
msgid "White"
msgstr "Fehér"

#. calendar@schorschii->settings-schema.json->text-color->description
msgid "Text color"
msgstr "Szövegszín"

#. calendar@schorschii->settings-schema.json->text-color->tooltip
msgid "Set the text color of this desklet."
msgstr "Asztalkalmazás szövegszínének beállítása."

#. calendar@schorschii->settings-schema.json->read-appointments->description
msgid "Show the number of today's appointments parsed from an ical file"
msgstr "Mutassa meg a mai találkozók számát (ical fájlból importált)"

#. calendar@schorschii->settings-schema.json->read-appointments->tooltip
msgid "Checking this box allows you to set an ical file to parse in the field below."
msgstr "Ennek a négyzetnek a bejelölésével az alábbi mezőben megadhat egy importálandó ical fájlt."

#. calendar@schorschii->settings-schema.json->ical-file->description
msgid "Path to ical file"
msgstr "Elérési útvonal az ical fájlhoz"

#. calendar@schorschii->settings-schema.json->ical-file->tooltip
msgid ""
"Select your ical file for reading the amount of appointments today.\n"
"\n"
"You can configure Lightning (the Thunderbird Add-On) to save your calendar in an ical file."
msgstr ""
"Válassza ki az ical fájlt a mai találkozók mennyiségének beolvasásához.\n"
"\n"
"Beállíthatja a Lightning-et (a Thunderbird kiegészítőjét), hogy a naptárat egy ical fájlba mentse."

#. calendar@schorschii->settings-schema.json->scale-size->description
msgid "Desklet size"
msgstr "Asztalkalmazás mérete"

#. calendar@schorschii->settings-schema.json->scale-size->tooltip
msgid "Increase or decrease the size of the calendar using this scale factor."
msgstr "Növelje vagy csökkentse a naptár méretét ezzel a léptéktényezővel."

#. calendar@schorschii->settings-schema.json->scale-size->units
msgid "scale factor"
msgstr "léptéktényező"

#. calendar@schorschii->settings-schema.json->use-custom-label->description
msgid "Use a custom desklet label"
msgstr "Egyéni asztalkalmazás címke használata"

#. calendar@schorschii->settings-schema.json->use-custom-label->tooltip
msgid "Checking this box allows you to set a custom label in the field below."
msgstr "Ennek a négyzetnek a bejelölésével egyéni címkét állíthat be az alábbi mezőben."

#. calendar@schorschii->settings-schema.json->custom-label->description
msgid "Custom desklet label"
msgstr "Egyéni asztalkalmazás címke"

#. calendar@schorschii->settings-schema.json->custom-label->tooltip
msgid ""
"Set your custom label here, e.g. for monitoring multiple ical files. This field is unavailable unless the checkbox "
"above is enabled."
msgstr ""
"Itt állítson be egyéni címkét, pl. több ical fájl megfigyelésére. Ez a mező csak akkor érhető el, ha a fenti négyzet "
"be van jelölve."

msgid "Appointment number color"
msgstr "Találkozó számának háttérszíne"

msgid "Start program on click of the desklet"
msgstr "Program indítása az asztalkalmazásra kattintva"

msgid "Start a program (e.g. Thunderbird)"
msgstr "Egy program indítása (pl. Thunderbird)"

msgid "The command entered below will be executed on mouse click of the desklet. So you can directly start Thunderbird."
msgstr ""
"Az alább megadott parancs végre lesz hajtva ha az asztalkalmazásra kattint. Így közvetlenül elindíthatja a "
"Thunderbird-öt."

msgid "Command line"
msgstr "Parancssor"

msgid "Set your program including parameters to start on click."
msgstr "Állítsa be rákattintással induló programját paramétereivel együtt."
