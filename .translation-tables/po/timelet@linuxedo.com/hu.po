#. metadata.json->description
msgid "A themeable desklet that shows the time"
msgstr "Időt megjelenítő témázható asztalkalmazás"

#. settings-schema.json->head0->description
msgid "Timelet Settings"
msgstr "Timelet beállítások"

#. settings-schema.json->themeName->description
msgid "Theme"
msgstr "Téma"

#. settings-schema.json->use24H->description
msgid "Use 24H"
msgstr "24 órás időformátum használata"

#. settings-schema.json->use24H->tooltip
msgid "Display time in 24H if the theme supports it"
