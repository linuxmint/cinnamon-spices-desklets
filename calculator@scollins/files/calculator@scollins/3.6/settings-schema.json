{
    "angleMode": {
        "type": "radiogroup",
        "description": "Angle Measure",
        "options": {
            "Degrees": 0,
            "Radians": 1
        },
        "default": 0
    },
    
    "rpn": {
        "type": "checkbox",
        "description": "Use Reverse-Polish Notation",
