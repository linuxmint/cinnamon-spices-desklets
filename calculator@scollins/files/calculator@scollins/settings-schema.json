{
    "style": {
        "type": "combobox",
        "default" : "dark",
        "description" : "Style",
        "options" : {
            "Light" : "light",
            "Dark": "dark"
        }
    },

    "angleMode": {
        "type": "radiogroup",
        "description": "Angle Measure",
        "options": {
            "Degrees": 0,
            "Radians": 1
        },
        "default": 0
    },

    "rpn": {
        "type": "checkbox",
        "description": "Use Reverse-Polish Notation",
        "default": false
    },

    "layout": {
        "type": "radiogroup",
        "description": "Style",
        "options": {
            "4-function": 1,
            "Scientific": 2
        },
        "default": 1
    },

    "precision": {
        "type": "spinbutton",
        "description": "Precision",
        "min": 6,
        "max": 14,
        "step": 1,
        "default": 10,
        "units": "digits"
    },

    "raiseKey": {
        "type": "keybinding",
        "description": "Raise to top shorcut key",
        "default": "<Super>c"
    }
}
