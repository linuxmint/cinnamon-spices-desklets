msgid "px"
msgstr "px"

#. settings-schema.json->cornerradius->description
msgid "Corner radius"
msgstr "Hoekradius"

#. settings-schema.json->cornerradius->tooltip
msgid "Radius for rounding the desklet's corners"
msgstr "Straal voor het afronden van de hoeken van de desklet"

#. settings-schema.json->display_mode->description
msgid "Display mode"
msgstr "Weergavemodus"

#. settings-schema.json->display_mode->options
msgid "normal"
msgstr "normaal"

#. settings-schema.json->display_mode->options
msgid "compact"
msgstr "compact"

